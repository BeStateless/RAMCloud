 GNUmakefile |    5 +++++
 1 file changed, 5 insertions(+)

diff --git a/GNUmakefile b/GNUmakefile
index 6952a3bb..482746e5 100644
--- a/GNUmakefile
+++ b/GNUmakefile
@@ -448,6 +448,11 @@ INSTALL_INCLUDES := \
     src/LogMetadata.h \
     src/MasterClient.h \
     src/Minimal.h \
+    src/MultiIncrement.h \
+    src/MultiOp.h \
+    src/MultiRead.h \
+    src/MultiRemove.h \
+    src/MultiWrite.h \
     src/Object.h \
     src/ObjectBuffer.h \
     src/ObjectRpcWrapper.h \
